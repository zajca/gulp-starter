{
  "name": "gulp-starter",
  "version": "0.1.0",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browserify-shim": "~3.4.1",
    "browserify": "~3.36.0",
    "coffeeify": "~0.6.0",
    "connect": "~2.14.3",
    "gulp-changed": "~0.3.0",
    "gulp-less": "~1.1.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-livereload": "~1.2.0",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp": "~3.6.0",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "^0.1.1",
    "css-sprite": "^0.7.0-beta3",
    "gulp-if": "^1.0.0",
    "gulp-plumber": "^0.6.1",
    "gulp-debug": "^0.3.0",
    "gulp-autoprefixer": "0.0.7",
    "gulp-rev": "^0.3.2",
    "gulp-minify-css": "^0.3.1",
    "gulp-rename": "^1.2.0",
    "angular": "^1.2.16",
    "angular-ui-router": "^0.2.10",
    "coffee-script": "^1.6.3",
    "gulp-ng-html2js": "^0.1.4",
    "gulp-minify-html": "^0.1.3",
    "gulp-concat": "^2.2.0",
    "gulp-uglify": "^0.2.1",
    "run-sequence": "^0.3.6",
    "gulp-clean": "^0.2.4",
    "longjohn": "^0.2.4",
    "gulp-inject": "^0.4.1"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.0"
  }
}
